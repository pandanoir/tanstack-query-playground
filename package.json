  "dependencies": {
    "@tanstack/react-query": "5.90.16",
    "@tanstack/react-router": "1.144.0",
    "json-server": "1.0.0-beta.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "concurrently": "9.2.1",
    "typescript": "5.9.3",
    "vite": "7.3.0"
  }
}
