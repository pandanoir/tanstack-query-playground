{
  "name": "my-vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"json-server db.json5\"",
    "build": "tsc -b && vite build",
    "lint": "biome lint ./src",
